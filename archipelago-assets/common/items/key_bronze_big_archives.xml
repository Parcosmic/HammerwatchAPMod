	</behavior>

	<sprite random-start="true" scale="16">
		<texture>items/key_bronze_big.png</texture>
    		<origin>8 13</origin>
    		<frame time="200">0 0 16 16</frame>
    		<frame time="200">16 0 16 16</frame>
    		<frame time="200">32 0 16 16</frame>
    		<frame time="200">16 0 16 16</frame>
    		<glow scale="16">
		<texture>items/key_bronze_big.png</texture>
		<frame time="200">0 16 16 16</frame>
		<frame time="200">16 16 16 16</frame>
		<frame time="200">32 16 16 16</frame>
		<frame time="200">16 16 16 16</frame>
    </glow>									
	</sprite>

	<collision shoot-through="true">
		<polygon>
			<point>-4 3</point>
			<point>-4 -7</point>
			<point>4 -7</point>
			<point>4 3</point>
		</polygon>
	</collision>
