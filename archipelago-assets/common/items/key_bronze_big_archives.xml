<item behavior="keymulti" minimap="menus/minimap.xml:bronzekey">
	<behavior>
		<dictionary>
			<entry name="pickup-text"><string>Archives Big Bronze Key!</string></entry>					
			<entry name="type"><int>0</int></entry>					
			<entry name="amount"><int>3</int></entry>
			<entry name="pickup-snd"><string>sound/misc.xml:take_key</string></entry>
			<entry name="pickup-particle"><string>effects/particles.xml:none</string></entry>
